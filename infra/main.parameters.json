{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "environmentName": {
        "value": "${AZURE_ENV_NAME}"
      },
      "location": {
        "value": "${AZURE_LOCATION}"
      },
      "appApiTodoExists": {
        "value": "${SERVICE_APP_API_TODO_RESOURCE_EXISTS=false}"
      },
      "appWebClientExists": {
        "value": "${SERVICE_APP_WEB_CLIENT_RESOURCE_EXISTS=false}"
      },
      "principalId": {
        "value": "${AZURE_PRINCIPAL_ID}"
      }
    }
}
