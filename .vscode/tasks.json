{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build-todo-api",
      "type": "process",
      "command": "dotnet",
      "args": [
        "build",
        "${workspaceFolder}/apps/App.Api.Todo/App.Api.Todo.csproj"
      ],
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "build-web-client",
      "type": "process",
      "command": "dotnet",
      "args": [
        "build",
        "${workspaceFolder}/apps/App.Web.Client/App.Web.Client.csproj"
      ],
      "problemMatcher": "$msCompile"
    }
  ]
}
